<div class="index-container">
    <button class="index-trigger" type="button">
        <div class="index-title-container" >
            <h2 class="index-title">Other Posts</h2>
            <p class="index-subtitle">Read my other posts here</p>
        </div>
    </button>
    <div class="index-content">
    <ul class="index-post-list">
        {% for post in collections.post %}
        <li>
            <a href="{{ post.url }}">{{ post.data.title }}</a>
            <hr>
        </li>
        {% endfor %}
    </ul>
    </div>
</div>
